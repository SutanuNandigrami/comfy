# ComfyUI Deployment Configuration
# Copy this file to .env and fill in your actual values

# === API Tokens ===

# HuggingFace Token (get from https://huggingface.co/settings/tokens)
HF_TOKEN=your_huggingface_token_here

# CivitAI API Token (get from https://civitai.com/user/account)
CIVITAI_TOKEN=your_civitai_token_here

# Ngrok Auth Token (get from https://dashboard.ngrok.com/get-started/your-authtoken)
NGROK_AUTHTOKEN=your_ngrok_token_here

# === Vast.AI Configuration (for CLI deployment) ===

# Vast.AI API Key (get from https://console.vast.ai/cli/)
VAST_API_KEY=your_vastai_api_key_here

# Google Drive Connection ID (from 'vastai show connections')
VAST_CONNECTION_ID=your_connection_id_here

# === Optional Settings ===

# ComfyUI Port
COMFYUI_PORT=8188

# Installation Mode (lite or full)
INSTALL_MODE=full

# GitHub Configuration
GITHUB_USER=SutanuNandigrami
GITHUB_REPO=comfy
